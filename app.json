{
  "expo": {
    "name": "DaUmHelp",
    "slug": "DaUmHelp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/LogoTeste1.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "splash": {
      "image": "./assets/LogoTeste5.png",
      "resizeMode": "contain",
      "backgroundColor": "#2C8AD8"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.arthupreiprei.DaUmHelp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/LogoTeste5.png",
        "backgroundColor": "#2C8AD8"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.arthupreiprei.DaUmHelp"
    },
    "web": {
      "favicon": "./assets/LogoTeste1.png"
    },
    "extra": {
      "eas": {
        "projectId": "ecd20bf8-1068-40d2-8828-6d84715cc0e4"
      }
    }
  }
}
